{
  "extends": "stylelint-config-sass-guidelines",
  "defaultSeverity": "warning",
  "plugins": [
    "stylelint-scss"
  ],
  "rules": {
    "selector-class-pattern": null,
    "block-no-empty": null,
    "scss/at-import-partial-extension-blacklist": null,
    "unit-whitelist": [
      "em",
      "rem",
      "s",
      "px"
    ]
  }
}